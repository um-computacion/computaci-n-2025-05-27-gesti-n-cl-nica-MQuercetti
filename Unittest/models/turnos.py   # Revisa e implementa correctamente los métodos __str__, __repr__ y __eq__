class Turno:
    def __init__(self, paciente, doctor, especialidad, fecha_hora):
        self.paciente = paciente
        self.doctor = doctor
        self.especialidad = especialidad
        self.fecha_hora = fecha_hora

    def __str__(self):
        return f"Turno: {self.paciente}, {self.doctor}, {self.especialidad}, {self.fecha_hora}"

    def __repr__(self):
        return f"<Turno(paciente={self.paciente}, doctor={self.doctor}, especialidad={self.especialidad}, fecha_hora={self.fecha_hora})>"

    def __eq__(self, other):
        if not isinstance(other, Turno):
            return NotImplemented
        return (self.paciente == other.paciente and
                self.doctor == other.doctor and
                self.especialidad == other.especialidad and
                self.fecha_hora == other.fecha_hora)